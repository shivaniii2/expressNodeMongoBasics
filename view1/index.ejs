<!DOCTYPE html>
<html lang="en">
 <%- include('./partials/head.ejs') %>
<body>
    <%- include('./partials/nav.ejs') %>
    
    <div class="newblogs">
        <% if(blogs.length >0){
          blogs.forEach((blog)=>{ %>
            <h2 class="blog-title"><%=blog.title %>  </h2>
            <p class="blog-snippet"> <%= blog.snippet %></p>
        <%  }) 
        }else{ %>
            <p>There are no blogs</p>
       <% } %>
        
    </div>
    
</body>
</html>